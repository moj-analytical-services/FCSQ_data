    "def get_count(Name,validation_checks):\n",
    "    count = pydb.read_sql_query(f\"select count(*) as count from {Name}\")['count'][0]\n",
    "    print (Name,count)\n",
    "    validation_checks = add_count(Name.split(\".\")[1],count,validation_checks)\n",
    "    return validation_checks"
   ]
  },
