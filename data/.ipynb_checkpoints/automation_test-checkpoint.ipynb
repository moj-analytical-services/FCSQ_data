   "source": [
    "def add_to_code_dict(old_name,new_name):\n",
    "    equivalence_dict = get_equivalence_dict()\n",
    "    equivalence_dict[old_name]=new_name\n",
    "    with open('table_equivalents.txt','w') as f:\n",
    "        f.write(json.dumps(equivalence_dict))\n",
    "    f.close()\n",
    "    "
