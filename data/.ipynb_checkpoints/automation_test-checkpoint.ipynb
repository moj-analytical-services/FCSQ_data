    "def get_equivalence_dict():\n",
    "    with open('table_equivalents.txt','r') as f:\n",
    "        data = f.read()\n",
    "    f.close()\n",
    "    equivalents_dict = json.loads(data)\n",
    "    return equivalents_dict"
   ]
  },
