    "def get_inputs():\n",
    "    table_name = input(\"table name (no spaces): \")\n",
    "    while len(table_name.split())>1:\n",
    "        table_name = input(\"table name (no spaces): \")\n",
    "    number = input(\"number: \")\n",
    "    desc = input(\"table description: \")\n",
    "    temp_flag = \"\"\n",
    "    while temp_flag not in ('T','F'):\n",
    "        temp_flag = input(\"Temporary table T/F: \").upper()\n",
    "    if temp_flag == 'T':\n",
    "        temp_flag = True\n",
    "    else:\n",
    "        temp_flag = False\n",
    "    return table_name,number,desc,temp_flag"
